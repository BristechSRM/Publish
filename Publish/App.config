<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings file="Publish.exe.secrets">
    <add key="serilog:minimum-level" value="Debug" />
    <add key="serilog:write-to:ColoredConsole" />
    <!-- Service Url-->
    <!-- Prod and Local Swarm-->
    <!--<add key="BaseUrl" value="http://*:8080" />-->
    <!-- Local Dev -->
    <add key="BaseUrl" value="http://*:8084" />
    
    <!-- Meetup Target Group-->
    <!-- Dev-->
    <add key="MeetupTargetGroup" value="Bris-testing"/>
    
    <!-- Other Service-->
    <!-- Prod and Local Swarm -->
    <!--<add key="SessionsServiceUrl" value="http://sessions:8080"/>-->
    <!-- Local Dev -->
    <add key="SessionsServiceUrl" value="http://localhost:8083" />
  </appSettings>
</configuration>
